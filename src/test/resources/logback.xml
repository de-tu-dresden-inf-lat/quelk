<configuration>

    <appender name="STDOUT"
              class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>
                %d{HH:mm:ss} %-5level %logger{36} - %msg%n
            </pattern>
        </encoder>
    </appender>

    <logger name="de.tu_dresden.epistemic_rewriter.TELHReasoner" level="INFO" />
    <logger name="de.tu_dresden.epistemic_rewriter.TemporalDatabaseModel" level="DEBUG" />

    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>

</configuration>